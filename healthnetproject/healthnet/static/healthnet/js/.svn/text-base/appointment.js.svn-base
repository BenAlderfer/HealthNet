$( document ).ready( function() {
    var date_time = $('#date_time');

    date_time.datetimepicker({
        locale: 'en',
        sideBySide: true,
        format: 'MMM DD YYYY, h:mm a',
        showClose: true,
        toolbarPlacement: 'bottom',
        icons:{
            close: 'glyphicon glyphicon-ok'
        }
    });
});